{
  "name": "vue-js",
  "version": "1.0.0",
  "description": "Serverless Cloud Vue.js template",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "cloud",
    "build": "cloud build",
    "lint": "vue-cli-service lint",
